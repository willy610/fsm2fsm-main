  "version": "0.2.0",
  "configurations": [
    {//(macOS/Unix) Debug stdinclient
      "name": "(macOS/Unix) Debug stdinclient",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/stdinclient",
      "args": ["--seed", "610"],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "lldb",
    },
    {//(Win) Debug stdinclient
      "name": "(Windows) Debug stdinclient",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/stdinclient.exe",
      "args": [
        "-seed",
        "123"
      ],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false
    }
  ]
}